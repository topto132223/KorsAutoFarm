local currentWalkSpeed = 50
local Slider = MainTab:CreateSlider({
    Name = "WalkSpeed",
    Range = {1, 350},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = currentWalkSpeed,
    Flag = "sliderws", 
    Callback = function(Value)
        currentWalkSpeed = Value
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = currentWalkSpeed
    end,
})

spawn(function()
    while true do
        wait(0.01)
        if game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = currentWalkSpeed
        end
    end
end)

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")

local orbsFolder = Workspace:WaitForChild("Orbs")
local MIN_DISTANCE = 50
local SAFE_DISTANCE = 10
local farmingSpeed = 1
local farmingActive = false

local function isPlayerSafe(player)
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
        return false
    end

    local myPosition = player.Character.HumanoidRootPart.Position

    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local otherPosition = otherPlayer.Character.HumanoidRootPart.Position
            local distance = (myPosition - otherPosition).Magnitude
            
            if distance < SAFE_DISTANCE then
                return false
            end
        end
    end
    
    return true
end

local function isPositionSafe(targetPosition, player)
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local otherPosition = otherPlayer.Character.HumanoidRootPart.Position
            local distance = (targetPosition - otherPosition).Magnitude
            
            if distance < SAFE_DISTANCE then
                return false
            end
        end
    end
    return true
end

local function teleportToRandomOrb()
    local player = Players.LocalPlayer
    
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local orbs = orbsFolder:GetChildren()
        if #orbs > 0 then
            local randomOrb
            local targetPosition
            local isSafe = false

            repeat
                randomOrb = orbs[math.random(1, #orbs)]
                targetPosition = randomOrb.Position + Vector3.new(0, 5, 0)
                isSafe = isPositionSafe(targetPosition, player)
            until isSafe

            player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)

            local distance = (player.Character.HumanoidRootPart.Position - randomOrb.Position).Magnitude
            if distance < MIN_DISTANCE then
                print("Collected: " .. randomOrb.Name)
                randomOrb:Destroy()
            end
        else
            warn("No available orbs in the Orbs folder.")
        end
    else
        warn("Player character is not loaded or HumanoidRootPart is missing.")
    end
end

local function startFarming()
    while farmingActive do
        if isPlayerSafe(Players.LocalPlayer) then
            wait(farmingSpeed)
            teleportToRandomOrb()
        else
            wait(1)
        end
    end
end

local Toggle = MainTab:CreateToggle({
    Name = "Auto Farm",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        farmingActive = Value
        if farmingActive then
            print("Auto farming started.")
            startFarming()
        else
            print("Auto farming stopped.")
        end
    end,
}) 

local Slider = MainTab:CreateSlider({
    Name = "Farming Speed",
    Range = {1, 5}, 
    Increment = 1, 
    Suffix = "Speed",
    CurrentValue = 1, 
    Flag = "sliderFarmSpeed", 
    Callback = function(Value)
        if Value == 1 then
            farmingSpeed = 1
        elseif Value == 2 then
            farmingSpeed = 0.7
        elseif Value == 3 then
            farmingSpeed = 0.4
        elseif Value == 4 then
            farmingSpeed = 0.1
        elseif Value == 5 then
            farmingSpeed = 0.06
        end
        print("Farming speed set to:", farmingSpeed)
    end,
}) 

local Button = MainTab:CreateButton({
    Name = "Give Vip and 2x Boost",
    Callback = function()
        print("Boosts activated.")

        local Players = game:GetService("Players")
        local player = Players.LocalPlayer

        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local humanoidRoot = player.Character.HumanoidRootPart
            local statsGui = humanoidRoot:FindFirstChild("StatsGui")

            if statsGui and statsGui:IsA("BillboardGui") then
                local mainFrame = statsGui:FindFirstChild("MainFrame")

                if mainFrame then
                    local zzvip = mainFrame:FindFirstChild("ZZVip")
                    if zzvip and zzvip:IsA("TextLabel") then
                        zzvip.Visible = true 
                    end
                end
            end

            local speedBoost = player:FindFirstChild("speedBoost")
            if speedBoost then
                speedBoost.Value = true
            end

            local tBoost = player:FindFirstChild("tBoost")
            if tBoost then
                tBoost.Value = true
            end

            local vipBoost = player:FindFirstChild("vipBoost")
            if vipBoost then
                vipBoost.Value = true
            end

            local slowedDown = player:FindFirstChild("SlowedDown")
            if slowedDown then
                slowedDown.Value = true
            end
        else
            warn("Player character is not loaded or HumanoidRootPart is missing.")
        end
    end,
})

